{
  "expo": {
    "name": "Taxi Conductor",
    "slug": "taxi_conductor",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#ffc107",
    "androidStatusBar": {
      "backgroundColor": "#FF9800",
      "barStyle": "light-content"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF9800"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#FF9800"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "tech.sinewave.driver",
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This is for showing the user's location on the map."
      }
    }
  }
}
